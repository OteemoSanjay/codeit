#!/bin/bash

websites=("https://cluster-console.dev.lib.harvard.edu" "https://deployment.dev.lib.harvard.edu" "https://logs.dev.lib.harvard.edu" "https://access-dev.lib.harvard.edu" "https://acorn-dev.lib.harvard.edu/records/list" "https://arclight-dev.lib.harvard.edu/" "https://aspace-dev.lib.harvard.edu/" "https://aspacepui-dev.lib.harvard.edu/" "https://booklabeler-dev.lib.harvard.edu/" "https://curiosity-dev.lib.harvard.edu" "https://drsmqsrv-dev.lib.harvard.edu/drs2_activemq/rest/heartbeat" "https://drs2-dev.hul.harvard.edu/drs2_webadmin/search" "https://embed-dev.lib.harvard.edu/" "https://fts-dev.lib.harvard.edu/fts/" "https://hgl-dev.lib.harvard.edu" "https://ids-dev.lib.harvard.edu" "https://imgconv-dev.lib.harvard.edu/health" "https://jobmon-dev.lib.harvard.edu" "https://jstor-dev.lib.harvard.edu" "https://listview-dev.lib.harvard.edu" "https://lts-pipelines-dev.lib.harvard.edu/home" "https://mps-dev.lib.harvard.edu/iiif/2/URN-3:HUL.OIS:101114808" "https://nrs-dev.lib.harvard.edu/URN-3:HUL.OIS:101114808:MANIFEST:2" "https://nrsadmin-dev.lib.harvard.edu/resources/search/advanced" "https://policyadmin-dev.lib.harvard.edu/policy/servlet/admin" "https://pds-dev.lib.harvard.edu/pds" "https://secureexit-dev.lib.harvard.edu"  "https://tools-dev.lib.harvard.edu/hdforms/retrieval" "https://viewer-dev.lib.harvard.edu/" "https://webservices-dev.lib.harvard.edu/rest/v3/hollis/marc/isbn/9780307269751?jsonp=foo")

for site in "${websites[@]}"

do

    status_code=$(curl -Is $site | head -n 1 | cut -d ' ' -f 2)

    echo "$site: $status_code"

done
