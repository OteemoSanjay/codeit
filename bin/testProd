#!/bin/bash

websites=("https://cluster-console.prod.lib.harvard.edu" "https://deployment.prod.lib.harvard.edu" "https://logs.prod.lib.harvard.edu/" "https://booklabeler.lib.harvard.edu/" "https://curiosity.lib.harvard.edu" "https://eadchecker.lib.harvard.edu/" "https://embed.lib.harvard.edu" "https://fts.lib.harvard.edu/fts/" "https://tools.lib.harvard.edu/hdforms/retrieval" "https://viewer.lib.harvard.edu")

for site in "${websites[@]}"

do

    status_code=$(curl -Is $site | head -n 1 | cut -d ' ' -f 2)

    echo "$site: $status_code"

done
